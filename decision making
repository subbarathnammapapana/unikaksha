#Greatest of two numbers
a=int(input())
b=int(input())
if(a>b):
    print(a,"is greater than",b)
elif(a<b):
    print(a,"is less than",b)
else:
    print(a,"is equal to ",b)
    
#Vowel Or Consonants
a= input("Enter a character: ")
if a in "aeiouAEIOU":
    print("Vowel")
elif a in "bBcCDdFfgGhHJjkKlLmMnNpPqQrRsStTvVwWxXyYz":
    print("Consonant")
else:
    print("not an alphabet")
    
#Grading System
marks=int(input())
if marks==100:
    print("grade-S")
elif marks>=90 and marks<=100:
    print("grade-A")
elif marks>=80 and marks<=90:
    print("grade-B")
elif marks>=70 and marks<=80:
    print("grade-C")
elif marks>=60 and marks<=70:
    print("grade-D")
elif marks>=50 and marks<=60:
    print("grade-E")
elif marks<=50:
    print("grade-F")
else:
    print("invalid input")

#Profit or Loss
x=float(input())
y=float(input())
z=y*12
print("Enter the price of a dozen mangoes")
print("Enter the price at which 1 mango is being sold")
if(x>z):
    print("Loss : RS.%.2f"%(x-z))
elif(z>x):
    print("Profit : RS.%.2f"%(z-x))
else:
    print("No profit nor loss")
    
#Fee Collection
print("Enter the student type")
s=(input())
if s=="MSDS":
    print("Enter tuition fee ")
    T=float(input())
    print("Enter bus fee")
    B=float(input())
    A=T+B
    print("The fees to be paid by the student is Rs.%.2f"%A)
elif s=="MSH":
    print("Enter tuition fee")
    T=float(input())
    print("Enter Hostel fee")
    H=float(input())
    C=T+H
    print("The fees to be paid by the student is Rs.%.2f"%C)
elif s=="MGSDS":
    print("Enter tuition fee")
    T=float(input())
    print("Enter bus fee")
    B=float(input())
    D=((150/100)*T+B)
    print("The fees to be paid by the student is Rs.%.2f"%D)
elif s=="MGSH":
    print("Enter tuition fee")
    T=float(input())
    print("Enter Hostel fee")
    H=float(input())
    E=((150/100)*T+H)
    print("The fees to be paid by the student is Rs.%.2f"%E)
    
#Y2K Problem Detector
print("enter year of birth")
a=int(input())
print("enter current year")
b=int(input())
if(b<a):
    a=100-a
    c=a+b
    print("your age is",c)
else:
    print("your age is",b-a)
    
#Lab Allocation
print("enter x")
L1=int(input())
print("enter y")
L2=int(input())
print("enter z")
L3=int(input())
if(L1<L2 and L1<L3):
    print("L1 has the minimal seating capacity")
elif(L2<L3):
    print("L2 has the minimal seating capacity")
else:
    print("L3 has the minimal seating capacity")
    
