#Budget
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
p=A*350.34
q=B*230.90
r=C*190.55
s=D*125.30
t=E*180.90
m=p+q+r+s+t
if(m<=15000):
    print("yes")
else:
    print("no")
    
#Sece Dining
a=input()
b=int(input())
if a=="front" and b==1 or a=="rear" and b==2:
    print("Left Handed")
elif a=="front" and b==2 or a=="rear" and b==1:
    print("Right Handed")
else:
    print("Invalid Input")
    
#Intersection of a Circle
import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
a=math.sqrt((x1-x2)**2+(y1-y2)**2)
b=r1+r2
if a==b:
    print("the circles are tangential")
elif(a<b):
    print("the circles overlap")
else:
    print("the circles do not overlap")
    
#Microwave Oven
print("Enter the number of items")
a=int(input())
print("Enter the single item heating time")
b=float(input())
r=b+(50/100)*b
s=a+(2*b)
if a==2:
    print("The recommended heating time is %.2f"%r)
elif a==3:
    print("The recommended heating time is %.2f"%s)
else:
    print("Number of items is more")
